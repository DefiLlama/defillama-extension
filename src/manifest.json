{
  "name": "DefiLlama",
  "description": "issa llama whirl!",
  "author": "DefiLlama Team",
  "version": "0.0.2.1",
  "manifest_version": 3,
  "offline_enabled": true,
  "icons": {
    "128": "assets/icons/defillama-logo-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab/index.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content-script/index.ts"],
      "css": ["generated:content-script/style.css"]
    }
  ],
  "permissions": ["topSites", "storage", "tabs"]
}
